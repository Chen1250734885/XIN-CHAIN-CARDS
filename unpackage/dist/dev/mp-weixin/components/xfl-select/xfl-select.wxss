.popper__arrow{
				 -webkit-transform: translateX(-400%);
				         transform: translateX(-400%);
				 position: absolute;
				 display: block;
				 width: 0;
				 height: 0;
				 border-color: transparent;
				 border-style: solid;
				 border-width: 6px;
				 -webkit-filter: drop-shadow(0 2px 12px rgba(0,0,0,.03));
